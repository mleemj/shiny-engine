        <parameter xmlns:pns="http://lets.go.mars" xmlns:tns="http://www.w3.org/2001/XMLSchema" qname="pns:bookauthor" type="tns:string"/>
        <parameter xmlns:pns="http://lets.go.mars" xmlns:tns="http://www.w3.org/2001/XMLSchema" qname="pns:bookprice" type="tns:int"/>
      </operation>
      <parameter name="allowedMethods" value="getBook"/>

      <typeMapping xmlns:ns="http://butter.peanut.fried" qname="ns:Book" type="java:fried.peanut.butter.Book" serializer="org.apache.axis.encoding.ser.BeanSerializerFactory" deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory" encodingStyle=""/>
