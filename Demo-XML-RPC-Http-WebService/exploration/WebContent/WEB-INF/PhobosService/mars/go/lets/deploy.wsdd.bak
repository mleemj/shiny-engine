<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from PhobosService WSDL service -->

  <service name="Phobos" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://lets.go.mars"/>
      <parameter name="wsdlServiceElement" value="PhobosService"/>
      <parameter name="schemaQualified" value="http://lets.go.mars,http://butter.peanut.fried"/>
      <parameter name="wsdlServicePort" value="Phobos"/>
      <parameter name="className" value="mars.go.lets.PhobosSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Phobos"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getBook" qname="operNS:getBook" xmlns:operNS="http://lets.go.mars" returnQName="retNS:getBookReturn" xmlns:retNS="http://lets.go.mars" returnType="rtns:Book" xmlns:rtns="http://butter.peanut.fried" soapAction="" >
        <parameter qname="pns:booktitle" xmlns:pns="http://lets.go.mars" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:bookauthor" xmlns:pns="http://lets.go.mars" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:bookprice" xmlns:pns="http://lets.go.mars" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getBook"/>

      <typeMapping
        xmlns:ns="http://butter.peanut.fried"
        qname="ns:Book"
        type="java:fried.peanut.butter.Book"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
